{
  "compilerOptions": {
      "outDir": "dist",
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "esModuleInterop": true,
      "allowJs": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "downlevelIteration": true
  }
}
